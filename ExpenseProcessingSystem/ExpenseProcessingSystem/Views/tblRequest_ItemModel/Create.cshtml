@model ExpenseProcessingSystem.Models.Gbase.tblRequest_ItemModel

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Create</title>
</head>
<body>

<h4>tblRequest_ItemModel</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="SequenceNo" class="control-label"></label>
                <input asp-for="SequenceNo" class="form-control" />
                <span asp-validation-for="SequenceNo" class="text-danger"></span>
            </div>
            <div class="form-group">
                <div class="checkbox">
                    <label>
                        <input asp-for="ReturnFlag" /> @Html.DisplayNameFor(model => model.ReturnFlag)
                    </label>
                </div>
            </div>
            <div class="form-group">
                <label asp-for="Command" class="control-label"></label>
                <input asp-for="Command" class="form-control" />
                <span asp-validation-for="Command" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ScreenCapture" class="control-label"></label>
                <input asp-for="ScreenCapture" class="form-control" />
                <span asp-validation-for="ScreenCapture" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="RequestID" class="control-label"></label>
                <select asp-for="RequestID" class ="form-control" asp-items="ViewBag.RequestID"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

</body>
</html>
