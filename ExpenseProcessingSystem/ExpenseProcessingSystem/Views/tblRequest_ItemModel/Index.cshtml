@model IEnumerable<ExpenseProcessingSystem.Models.Gbase.tblRequest_ItemModel>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.SequenceNo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ReturnFlag)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Command)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ScreenCapture)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Request_Details)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.SequenceNo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ReturnFlag)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Command)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ScreenCapture)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Request_Details.RequestID)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ItemID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ItemID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ItemID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
</body>
</html>
