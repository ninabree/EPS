
@{
    ViewData["Title"] = "DMRejAccountGroup";
    Layout = "~/Views/Shared/_ModalLayout.cshtml";
}
@using ExpenseProcessingSystem.ViewModels
@model List<DMAccountGroupViewModel>
    <div id="table">
        @using (Html.BeginForm("RejAccountGroup", "Home", FormMethod.Post, new { @class = "validate-form", @id = "tsForm" }))
        {
            <table class="table table-bordered editable-th">
                <tr>
                    <th>Account Group Name</th>
                    <th>Account Group Code</th>
                    <th>Status</th>
                </tr>
                @if (Model != null)
                {
                    @for (int i = 0; i < Model.Count(); i++)
                    {
                        <tr>
                            <td>@Html.TextBoxFor(item => item[i].AccountGroup_Name, new { @class = "w-full", @readonly = "readonly" })</td>
                            <td>@Html.TextBoxFor(item => item[i].AccountGroup_Code, new { @class = "w-full", @readonly = "readonly" })</td>
                            <td>@Html.TextBoxFor(item => item[i].AccountGroup_Status_Name, new { @class = "w-full", @readonly = "readonly" })</td>
                            @Html.HiddenFor(item => item[i].AccountGroup_MasterID)
                            @Html.HiddenFor(item => item[i].AccountGroup_Creator_ID)
                        </tr>
                    }
                }
            </table>
        }
    </div>
