
@{
    ViewData["Title"] = "DMRejCheck";
    Layout = "~/Views/Shared/_ModalLayout.cshtml";
}

@using ExpenseProcessingSystem.ViewModels
@model List<DMCheckViewModel>
<div id="table" class="">
    @using (Html.BeginForm("RejCheck", "Home", FormMethod.Post, new { @class = "validate-form", @id = "tsForm" }))
    {
        <table class="table table-bordered ">
            <tr>
                <th>Check Input Date</th>
                <th>Check Series From</th>
                <th>Check Series To</th>
                <th>Check Bank Information</th>
                <th>Status</th>
            </tr>
            @if (Model != null)
            {
                @for (int i = 0; i < Model.Count(); i++)
                {
                    <tr>
                        <td>@Html.TextBoxFor(item => item[i].Check_Input_Date, new { @readonly = true, @class = "w-full" })</td>
                        <td>@Html.TextBoxFor(item => item[i].Check_Series_From, new { @class = "w-full", @readonly = "readonly" })</td>
                        <td>@Html.TextBoxFor(item => item[i].Check_Series_To, new { @class = "w-full", @readonly = "readonly" })</td>
                        <td>@Html.TextBoxFor(item => item[i].Check_Bank_Info, new { @class = "w-full", @readonly = "readonly" })</td>
                        <td>@Html.TextBoxFor(item => item[i].Check_Status, new { @class = "w-full", @readonly = "readonly" })</td>
                        @Html.HiddenFor(item => item[i].Check_MasterID)
                        @Html.HiddenFor(item => item[i].Check_Creator_ID)
                    </tr>
                }
            }
        </table>
    }
</div>